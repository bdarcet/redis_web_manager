<div class="container">
  <div class="row">
    <div class="col-8">
      Graph
    </div>
    <div class="col-4">
      <ul class="list-group list-group-flush">
        <li class="list-group-item d-flex justify-content-between align-items-center">
          Info
        </li>
        <% @informations.each do |data| %>
          <li class="list-group-item d-flex justify-content-between align-items-center">
            <%= data[:name] %>
            <span><%= data[:value] %></span>
          </li>
        <% end %>
      </ul>
    </div>
  </div>
</div>
